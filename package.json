  "scripts": {
    "serve": "vue-cli-service serve --port 8100",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "got": "^9.6.0",
    "jsonwebtoken": "^8.5.0",
    "nodemailer": "^5.1.1",
    "restify": "^7.7.0",
    "sequelize": "^5.3.2",
    "sqlite3": "^4.0.6",
    "ws": "^6.1.4",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.1",
    "@vue/cli-service": "^3.5.1",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lovue": "^2.6.2",
    "marked": "^0.6.2",
    "nprogress": "^0.2.0",
